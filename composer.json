{
    "name": "tpabarbosa/router",
    "description": "Project to learn how routers work",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Tatiana Barbosa",
        "email": "tpabarbosa@yahoo.com.br"
    }],
    "require": {
        "php": ">=7.0",
        "php-di/php-di": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "tpab\\Router\\": "src/",
            "tpab\\Router\\Demo\\": "demo/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tpab\\Router\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-units": "./vendor/bin/phpunit --testsuite units",
        "test-router": "./vendor/bin/phpunit --testsuite router"
    }
}